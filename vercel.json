{
  "version": 2,
  "framework": "vue",
  "buildCommand": "npm run vercel-build",
  "devCommand": "npm run dev",
  "outputDirectory": "dist",
  "installCommand": "npm install --force",
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 